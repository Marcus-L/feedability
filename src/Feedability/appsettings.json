{
	"Logging": {
		"IncludeScopes": false,
		"LogLevel": {
			"Default": "Debug",
			"System": "Information",
			"Microsoft": "Information"
		}
	},
	"IpRateLimiting": {
		"EnableEndpointRateLimiting": true,
		"StackBlockedRequests": false,
		"HttpStatusCode": 429,
		"GeneralRules": [
			{
				"Endpoint": "*:/api/fullfeed",
				"Period": "60s",
				"Limit": 5
			},
			{
				"Endpoint": "*:/api/fullfeed",
				"Period": "1d",
				"Limit": 15
			}
		]
	},
	"FullFeed": {
		"MaxFeedEntriesProcessed":  5
	}
}
